<div class="row row-cols-3  g-2 g-lg-3 py-4" style="align-items: center;">
    <% if(healthCare != '') { %>
        <% healthCare.forEach(function(healthProduct , index) { %>
            <% let imageName =  healthProduct.name+".jpg"%>
            <a href="/healthCare/<%- healthProduct.id %>"class="col text-center category_link px-5">
                <div class="category_display shadow" style="background-color: #e9eaec;">
                    <!-- <img src="uploads/Amla Candy.jpg" alt="Medicines" loading="lazy"> -->
                    <img src="uploads/<%-imageName%>" alt="<% healthProduct.name %>">
                </div>
                <figcaption><%- healthProduct.name %></figcaption>
                <div class="pt-1"></div>
            </a>
        <% }) %>
    <% } %>
</div>



